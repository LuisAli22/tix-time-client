<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<GridPane alignment="center" hgap="10" prefHeight="282.0" prefWidth="445.0" vgap="10"
          xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.github.tix_measurements.time.client.ui.Controller">
    <padding>
        <Insets bottom="10" left="25" right="25" top="25"/>
    </padding>
    <Text text="Iniciar sesión" GridPane.columnSpan="2" GridPane.rowIndex="0"/>

    <Label text="Email" GridPane.columnIndex="0" GridPane.rowIndex="1"/>

    <TextField GridPane.columnIndex="1" GridPane.rowIndex="1"/>

    <Label text="Contraseña" GridPane.columnIndex="0" GridPane.rowIndex="2"/>

    <PasswordField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <Button mnemonicParsing="false" text="Enviar" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
    <Hyperlink text="¿Olvidó su contraseña?" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
    </rowConstraints>
</GridPane>
